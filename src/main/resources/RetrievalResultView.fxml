<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.RetrievalResultController">
    <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Retrieval Results">
         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Text>
      <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="SOMETIMES">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Results" underline="true" />
                  <TableView fx:id="resultTableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Display Entities" underline="true" />
                  <TextField fx:id="docNumberTextField" promptText="Doc Number">
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </TextField>
                  <Button fx:id="displayEntitiesButton" mnemonicParsing="false" onAction="#displayEntities" text="Display">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveResults" text="SaveResults">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="10.0" top="5.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
        <Button fx:id="closeButton" mnemonicParsing="false" text="Close">
            <VBox.margin>
                <Insets bottom="10.0" top="10.0" />
            </VBox.margin></Button>
    </children>
</VBox>
